SELECT package_name,       datetime(record_time / 1000, 'unixepoch', 'localtime') AS record_time,       time(foreground_time_usage / 1000, 'unixepoch') AS foreground_time,       datetime(last_time_use/1000, 'unixepoch', 'localtime') AS last_time_use2,       downloaded_data / 1024 AS downloaded_kb,       uploaded_data / 1024 AS uploaded_kb,       was_foreground  FROM Table_applications_activity